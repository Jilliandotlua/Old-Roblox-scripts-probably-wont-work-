local AntiAfk = Gamee:CreateToggle({
	Name = "AFK Farm (Free XP and Survivor Badges)",
	CurrentValue = false,
	Flag = "Toggle854",
	Callback = function(Value)
		if Value == true then
				repeat wait(1) antiafk() until Rayfield.Flags["Toggle854"].CurrentValue == false
			else
			
		end
	end,
})

