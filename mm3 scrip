local WalkSpeed = Player:CreateToggle({
	Name = "CFrame Walkspeed",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(Value)
		getgenv().cframe = Value
		getgenv().sped = Value
	end,
})


local WalkSpeedSpeed = Player:CreateSlider({
	Name = "CFrame WalkSpeed Speed",
	Range = {1, 20},
	Increment = 1,
	Suffix = "Speed",
	CurrentValue = 10,
	Flag = "Slider1",
	Callback = function(Value)
		speedvalue = Value / 10
	end,
})

local JumpPower = Player:CreateSlider({
	Name = "Jump Power",
	Range = {50, 200},
	Increment = 5,
	Suffix = "Power",
	CurrentValue = 50,
	Flag = "Slider1",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end,
})


local Noclip = Player:CreateToggle({
	Name = "Noclip",
	CurrentValue = false,
	Flag = "Toggle1",
	Callback = function(Value)
	pcall(function()
		if Value == true then
			Stepped = game:GetService("RunService").Stepped:Connect(function()
					for a, b in pairs(Workspace:GetChildren()) do
						if b.Name == game.Players.LocalPlayer.Name then
							for i, o in pairs(Workspace[game.Players.LocalPlayer.Name]:GetChildren()) do
								if o:IsA("BasePart") then
									o.CanCollide = false
								end end end end
					
			end)
	
			else
			Stepped:Disconnect()
				for e,a in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
					if a:IsA("BasePart") then
						a.CanCollide = true
					end
				end
			end
		end)
	end,
})

