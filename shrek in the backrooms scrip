]])

-- Player
local Character = PlayerSec:AddSection({
	Name = "Character"
})

Character:AddSlider({
	Name = "WalkSpeed",
	Min = 0,
	Max = 200,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		Player.Character.Humanoid.WalkSpeed = Value
	end    
})

Character:AddSlider({
	Name = "JumpPower",
	Min = 0,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 10,
	ValueName = "Power",
	Callback = function(Value)
		Player.Character.Humanoid.JumpPower = Value
	end    
})

Character:AddToggle({
	Name = "Noclip",
	Default = false,
	Callback = function(Value)
		getgenv().Noclip = Value
	end    
})

-- Main

local AutoFarm = Main:AddSection({
	Name = "AutoFarm"
})

local Complete = Main:AddSection({
	Name = "Complete"
})


local Other = Main:AddSection({
	Name = "Other"
})

AutoFarm:AddToggle({
	Name = "AutoFarm Coins",
	Default = false,
	Flag = "autofarm",
	Callback = function(Value)
		if Value == true then
			repeat wait(OrionLib.Flags["farmdelay"].Value / 10) AutoFarmmCoin() until OrionLib.Flags["autofarm"].Value == false
			
		end
	end
})

AutoFarm:AddSlider({
	Name = "Coin AutoFarm Delay",
	Min = 1,
	Max = 50,
	Default = 2,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Example: 1 = .1, 10 = 1",
	Flag = "farmdelay",
	Callback = function(Value)
		
	end    
})

AutoFarm:AddToggle({
	Name = "AutoFarm Searchables",
	Default = false,
	Flag = "autofarmsearch",
	Callback = function(Value)
		if Value == true then
			repeat wait(OrionLib.Flags["farmdelaysearch"].Value) AutoFarmmSearch() until OrionLib.Flags["autofarmsearch"].Value == false
			
		end
	end
})

AutoFarm:AddSlider({
	Name = "Search AutoFarm Delay",
	Min = 1,
	Max = 60,
	Default = 1,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Seconds",
	Flag = "farmdelaysearch",
	Callback = function(Value)
		
	end    
})

Complete:AddButton({
	Name = "Complete All Levels",
	Callback = function()
      	CompleteAllLevels()
  	end    
})

Complete:AddSlider({
	Name = "Search AutoFarm Delay",
	Min = 1,
	Max = 50,
	Default = 4,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Example: 1 = .1 , 10 = 1",
	Flag = "completedelay",
	Callback = function(Value)
		
	end    
})

Other:AddButton({
	Name = "Delete All Stains",
	Callback = function()
      	for i,v in pairs(game.Workspace.Stains:GetChildren()) do
      		v:Destroy()
      	end
  	end    
})



